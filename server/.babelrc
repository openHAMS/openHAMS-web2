{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": { "node": "current" }
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "packagejson",
                "alias": {
                    "^~/(.+)": "./\\1",
                    "^@/(.+)": "./src/\\1",
                    "^Models/(.+)": "./src/models/\\1"
                }
            }
        ]
    ]
}
