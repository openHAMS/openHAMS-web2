{
    "env": {
        "node": false,
        "browser": true
    },
    "extends": [
        "plugin:vue/recommended",
        "plugin:import/warnings"
    ],
    "globals": {
        "process": false
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/html-indent": [
            "warn",
            4
        ]
    }
}
